   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            category="org.eclipse.vjet.eclipse.propertyPage"
            class="org.eclipse.vjet.eclipse.internal.debug.ui.pref.VjetDebugPrefPage"
            id="org.eclipse.vjet.eclipse.propertyPage.debug"
            name="%Debug.PropertyPage">
            	<enabledWhen>
		        <adapt type="org.eclipse.core.resources.IProject">
		          <test property="org.eclipse.core.resources.projectNature" value="org.eclipse.vjet.core.nature"/>
		        </adapt>         
		      </enabledWhen>
      </page>
   </extension>
   
   <extension point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="org.eclipse.vjet.eclipse.internal.debug.ui.launcher.VjetLaunchShortcut"
            icon="icons/javascript.gif"
            id="org.eclipse.vjet.eclipse.debug.ui.launchShortcut"
            label="VJET JS application"
            modes="run, debug">
				<contextualLaunch>
					<enablement>
					<with variable="selection">
						<count value="1"/>
						<iterate>
							<or>
								<test property="org.eclipse.vjet.eclipse.debug.launchJS"/>
               	    			<test property="org.eclipse.vjet.eclipse.debug.launchHTML"/>
							</or>
						</iterate>
					</with>
					</enablement>
      			</contextualLaunch>
	</shortcut>
      <shortcut
            class="org.eclipse.vjet.eclipse.internal.debug.ui.launcher.VjetWebLaunchShortcut"
            icon="icons/javascript.gif"
            id="org.eclipse.vjet.eclipse.debug.ui.launchShortcut"
            label="VJET Web application"
            modes="run">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <or>
                        <test
                              property="org.eclipse.vjet.eclipse.debug.launchJS">
                        </test>
                        <test
                              property="org.eclipse.vjet.eclipse.debug.launchHTML">
                        </test>
                     </or>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>
   
  <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.eclipse.vjet.eclipse.internal.debug.ui.launchConf.VjetTabGroup"
            id="org.eclipse.vjet.eclipse.debug.ui.launchConfigurations.VjetJsGroup"
            type="org.eclipse.vjet.eclipse.launching.VjetLaunchConfigurationType">
         <launchMode
               description="Debug VJET JS application"
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </launchMode>
         <launchMode
               description="Run a VJET JS application"
               mode="run">
         </launchMode>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="org.eclipse.vjet.eclipse.internal.debug.ui.launchConf.VjetTabGroup"
            id="org.eclipse.vjet.eclipse.debug.ui.launchConfigurations.VjetWebAppGroup"
            type="org.eclipse.vjet.eclipse.launching.VjetWebLaunchConfigurationType">
         <!--<launchMode
               description="Debug VJET Web application"
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </launchMode>-->
         <launchMode
               description="Run a VJET Web application"
               mode="run">
         </launchMode>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="org.eclipse.vjet.eclipse.internal.debug.ui.html.HtmlTabGroup"
            id="org.eclipse.vjet.eclipse.debug.ui.launchConfigurations.HTMLTabGroup"
            type="org.eclipse.vjet.eclipse.launching.HTMLLaunchConfigurationType">
         <launchMode
               description="Run a VJET JS application from html"
               mode="run">
         </launchMode>
         <launchMode
               description="Run a VJO application from html"
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </launchMode>
      </launchConfigurationTabGroup>
   </extension>
   
   <extension
         point="org.eclipse.dltk.mod.ui.buildpathContainerPage">
      <buildpathContainerPath
            class="org.eclipse.vjet.eclipse.internal.debug.ui.interpreters.VjetInterpreterContainerWizardPage"
            id="org.eclipse.vjet.eclipse.launching.INTERPRETER_CONTAINER"
            name="VJET Interpreter Libraries"
            nature="org.eclipse.vjet.core.nature">
      </buildpathContainerPath>
   </extension>
   
     <extension point="org.eclipse.ui.editorActions">
    <editorContribution
      targetID="org.eclipse.vjet.ui.VjetJsEditor"
      id="org.eclipse.vjet.eclipse.debug.VjoEditor.BreakpointRulerActions">

      <action
        label="Togle Breakpoint (label not used)"
        class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
        style="push"
        actionID="RulerDoubleClick"
        id="javascript.doubleClickBreakpointAction" />


    </editorContribution>
  </extension>
  
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.vjet.eclipse.internal.ui.editor.VjoEditor"
            class="org.eclipse.vjet.eclipse.internal.debug.ui.VjetEditorDebugAdapterFactory">
      <adapter type="org.eclipse.debug.ui.actions.IToggleBreakpointsTarget" />
      <adapter type="org.eclipse.debug.ui.actions.IRunToLineTarget" />
      </factory>
      <factory
            adaptableType="org.eclipse.dltk.mod.core.IMethod"
            class="org.eclipse.vjet.eclipse.internal.debug.ui.VjetEditorDebugAdapterFactory">
      <adapter type="org.eclipse.debug.ui.actions.IToggleBreakpointsTarget" />
      <adapter type="org.eclipse.debug.ui.actions.IRunToLineTarget" />
      </factory>
      <factory
            adaptableType="org.eclipse.wst.sse.ui.StructuredTextEditor"
            class="org.eclipse.vjet.eclipse.internal.debug.ui.html.HtmlEditorDebugAdapterFactory">
         <adapter
               type="org.eclipse.debug.ui.actions.IToggleBreakpointsTarget">
         </adapter>
         <adapter
               type="org.eclipse.debug.ui.actions.IRunToLineTarget">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.dltk.mod.ui.editorTextHovers">
      <hover
            class="org.eclipse.vjet.eclipse.internal.debug.ui.VjetDebugHover"
            id="org.eclipse.vjet.eclipse.internal.debug.ui.VjetDebugHover"
            label="%VjetVariableHoverLabel">
      </hover>
   </extension>
   <extension
         point="org.eclipse.debug.ui.debugModelPresentations">
      <debugModelPresentation
            class="org.eclipse.vjet.eclipse.internal.debug.ui.VjetDebugModelPresentation"
            id="org.eclipse.vjet.eclipse.debug.vjetModel">
      </debugModelPresentation>
   </extension>

  <extension point="org.eclipse.ui.console.consoleFactories">
    <consoleFactory
      class="org.eclipse.vjet.eclipse.internal.console.ui.VjetConsoleFactory"
      label="%TclConsole.Console" />
  </extension>

  <extension point="org.eclipse.dltk.mod.console.ui.scriptConsole">
    <scriptConsole
      class="org.eclipse.vjet.eclipse.internal.console.ui.VjetConsoleFactory"
      natureID="org.eclipse.vjet.core.nature" />
