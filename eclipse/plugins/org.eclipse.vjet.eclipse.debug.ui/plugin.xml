 <extension point="org.eclipse.ui.perspectiveExtensions">
    <perspectiveExtension
      targetID="org.eclipse.vjet.eclipse.ui.JavascriptPerspective">
      <actionSet id="org.eclipse.debug.ui.breakpointActionSet"></actionSet>
    </perspectiveExtension>
  </extension>

  <extension point="org.eclipse.ui.menus">
    <menuContribution locationURI="menu:org.eclipse.debug.ui.VariableView">
      <menu
        label="%variablesViewMenu.name"
        id="org.eclipse.vjet.eclipse.debug.ui.menu.VariableView" />
    </menuContribution>
    <menuContribution locationURI="menu:org.eclipse.vjet.eclipse.debug.ui.menu.VariableView">
      <command 
        commandId="org.eclipse.vjet.eclipse.debug.ui.commands.toggleGlobalVariables"
        tooltip="%showGlobalVarsCommand.tooltip"     
        style="toggle" />
      <command 
        commandId="org.eclipse.vjet.eclipse.debug.ui.commands.toggleClassVariables"
        tooltip="%showClassVarsCommand.tooltip"      
        style="toggle" />       
      <command 
        commandId="org.eclipse.vjet.eclipse.debug.ui.commands.toggleLocalVariables"
        tooltip="%showLocalVarsCommand.tooltip" 
        style="toggle" />                      
    </menuContribution>  
  </extension>
 
  <extension point="org.eclipse.ui.commands">
    <command
      id="org.eclipse.vjet.eclipse.debug.ui.commands.toggleGlobalVariables"
      description="%showGlobalVarsCommand.name"
      name="%showGlobalVarsCommand.description" />
    <command
      id="org.eclipse.vjet.eclipse.debug.ui.commands.toggleClassVariables"
      description="%showClassVarsCommand.name"
      name="%showClassVarsCommand.description" />
    <command
      id="org.eclipse.vjet.eclipse.debug.ui.commands.toggleLocalVariables"
      description="%showLocalVarsCommand.name"
      name="%showLocalVarsCommand.description" />
    <command
          categoryId="org.eclipse.debug.ui.category.run"
          description="Debug As VJET"
          id="org.eclipse.vjet.eclipse.debug.ui.launchShortcut.debug"
          name="Debug As VJET">
    </command>
    <command
          categoryId="org.eclipse.debug.ui.category.run"
          description="Run As VJET"
          id="org.eclipse.vjet.eclipse.debug.ui.launchShortcut.run"
          name="Run As VJET">
    </command>      
  </extension>
 
   <extension point="org.eclipse.debug.core.watchExpressionDelegates">
    <watchExpressionDelegate
      debugModel="org.eclipse.vjet.eclipse.debug.vjetModel"
      delegateClass="org.eclipse.dltk.mod.internal.debug.core.model.ScriptWatchExpressionDelegate">
    </watchExpressionDelegate>
  </extension>
 
   <extension point="org.eclipse.debug.ui.debugModelContextBindings">
    <modelContextBinding
      contextId="org.eclipse.dltk.mod.debug.ui.debugging"
      debugModelId="org.eclipse.vjet.eclipse.debug.vjetModel" />
  </extension>
 
  <extension point="org.eclipse.ui.actionSetPartAssociations">
    <actionSetPartAssociation
      targetID="org.eclipse.debug.ui.launchActionSet">
      <part id="org.eclipse.dltk.mod.ui.ScriptExplorer"></part>
      <part id="org.eclipse.vjet.ui.VjetJsEditor"></part>
    </actionSetPartAssociation>
  </extension>
  
 <extension
       point="org.eclipse.ui.popupMenus">
    <viewerContribution
          id="org.eclipse.dltk.mod.debug.ui.DebugPopupActions"
          targetID="#VjoEditorContext">
      <visibility>
        <and>
          <systemProperty
            name="org.eclipse.dltk.mod.debug.ui.debuggerActive"
            value="true">
          </systemProperty>
          <objectClass name="org.eclipse.jface.text.ITextSelection">
          </objectClass>
        </and>
      </visibility>
       <action
             class="org.eclipse.dltk.mod.internal.debug.ui.actions.ScriptWatchAction"
             icon="/icons/watch_exp.gif"
             id="org.eclipse.vjet.eclipse.debug.ui.ScriptWatch"
             label="%ScriptWatch.label"
             menubarPath="additions">
       </action>
       <action
             class="org.eclipse.dltk.mod.internal.debug.ui.actions.PopupScriptInspectAction"
             definitionId="org.eclipse.dlkt.debug.ui.commands.ScriptInspect"
             icon="/icons/insp_sbook.gif"
             id="org.eclipse.vjet.eclipse.debug.ui.ScriptInspect"
             label="%ScriptInspect.label"
             menubarPath="additions">
       </action>
       <action
             class="org.eclipse.dltk.mod.internal.debug.ui.actions.PopupScriptDisplayAction"
             definitionId="org.eclipse.dlkt.debug.ui.commands.ScriptDisplay"
             icon="/icons/disp_sbook.gif"
             id="org.eclipse.vjet.eclipse.debug.ui.ScriptDisplay"
             label="%ScriptDisplay.label"
             menubarPath="additions">
       </action>
    </viewerContribution>
    <objectContribution
            objectClass="org.eclipse.dltk.mod.core.IMethod"
            id="org.eclipse.vjet.debug.ui.MethodBreakpointActions">
          <filter
        name="org.eclipse.vjet.ui.method"
        value="true">
      </filter>
         <action
               class="org.eclipse.vjet.eclipse.internal.debug.ui.actions.ToggleMethodBreakpointActionDelegate"
               enablesFor="+"
               helpContextId="manage_method_breakpoint_action_context"
               icon="icons/full/obj16/brkp_obj.gif"
               id="org.eclipse.vjet.debug.ui.MethodBreakpoint"
               label="%manageMethodBreakpointAction.label"
               menubarPath="additions">
         </action>
      </objectContribution>
    
 </extension>
    <extension point="org.eclipse.dltk.mod.debug.ui.language">
    <language
      class="org.eclipse.vjet.eclipse.internal.debug.ui.VjoDebugUILanguageToolkit"
      nature="org.eclipse.vjet.core.nature"/>
  </extension>
  
   <extension
         point="org.eclipse.dltk.mod.ui.actionFilterTester">
      <actionFilterTester
            class="org.eclipse.vjet.eclipse.internal.debug.ui.actions.MethodActionFilterTest"
            id="org.eclipse.vjet.ui.method">
      </actionFilterTester>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.wst.sse.ui.StructuredTextEditor"
            class="org.eclipse.vjet.eclipse.internal.debug.ui.html.HtmlEditorDebugAdapterFactory">
         <adapter
               type="org.eclipse.debug.ui.actions.IToggleBreakpointsTarget">
         </adapter>
         <adapter
               type="org.eclipse.debug.ui.actions.IRunToLineTarget">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="org.eclipse.vjet.eclipse.debug.HtmlEditor.BreakpointRulerActions"
            targetID="org.eclipse.wst.html.core.htmlsource.source">
         <action
               actionID="RulerDoubleClick"
               class="org.eclipse.vjet.eclipse.internal.debug.ui.html.HtmlScriptToggleBreakpointActionDelegate"
               id="html.doubleClickBreakpointAction"
               label="Togle Breakpoint (label not used)"
               style="push">
         </action>
      </editorContribution>
   </extension>
     <extension point="org.eclipse.ui.handlers">
    <handler 
      commandId="org.eclipse.vjet.eclipse.debug.ui.commands.toggleGlobalVariables"
      class="org.eclipse.vjet.eclipse.internal.debug.ui.handlers.ToggleGlobalVariablesHandler" />
    <handler 
      commandId="org.eclipse.vjet.eclipse.debug.ui.commands.toggleClassVariables"
      class="org.eclipse.vjet.eclipse.internal.debug.ui.handlers.ToggleClassVariablesHandler" />
    <handler 
      commandId="org.eclipse.vjet.eclipse.debug.ui.commands.toggleLocalVariables"
      class="org.eclipse.vjet.eclipse.internal.debug.ui.handlers.ToggleLocalVariablesHandler" />      
  </extension>
     <extension
           point="org.eclipse.ui.bindings">
        <key
              commandId="org.eclipse.vjet.eclipse.debug.ui.launchShortcut.debug"
              contextId="org.eclipse.ui.globalScope"
              schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
              sequence="M3+M2+D V">
        </key>
        <key
              commandId="org.eclipse.vjet.eclipse.debug.ui.launchShortcut.run"
              contextId="org.eclipse.ui.globalScope"
              schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
              sequence="M3+M2+X V">
        </key>
     </extension>
     <extension
           point="org.eclipse.ui.views">
        <view
              category="org.eclipse.vjet.eclipse.ui.javascript.browsing"
              class="org.eclipse.vjet.eclipse.internal.debug.ui.views.RunningScriptView"
              icon="platform:/plugin/org.eclipse.vjet.eclipse.ui/icons/full/eview16/javascript_persp.gif"
              id="org.eclipse.vjet.eclipse.debug.ui.runningScriptView"
              name="%runningScriptView.name"
              restorable="true">
        </view>
     </extension>
     <extension
           point="org.eclipse.ui.perspectiveExtensions">
        <perspectiveExtension
              targetID="org.eclipse.debug.ui.DebugPerspective">
           <viewShortcut
                 id="org.eclipse.vjet.eclipse.debug.ui.runningScriptView">
           </viewShortcut>
           <view
                 id="org.eclipse.vjet.eclipse.debug.ui.runningScriptView"
                 minimized="false"
                 ratio="0.5"
                 relationship="stack"
                 relative="org.eclipse.debug.ui.VariableView"
                 visible="true">
           </view>
        </perspectiveExtension>
     </extension>
     <extension
           point="org.eclipse.dltk.mod.debug.smartStepEvaluator">
        <evaluator
              class="org.eclipse.vjet.eclipse.internal.debug.ui.VjetSmartStepEvaluator"
              nature="org.eclipse.vjet.core.nature"
              priority="0">
        </evaluator>
     </extension>
     <extension
           point="org.eclipse.core.runtime.preferences">
        <initializer
              class="org.eclipse.vjet.eclipse.internal.debug.ui.VjetDebugPreferenceInitializer">
        </initializer>
     </extension>
     <extension
           point="org.eclipse.debug.ui.variableValueEditors">
        <variableValueEditor
              class="org.eclipse.vjet.eclipse.internal.debug.ui.actions.VjetScriptVariableValueEditor"
              modelId="org.eclipse.vjet.eclipse.debug.vjetModel">
        </variableValueEditor>
     </extension>
   
   
 </plugin>
